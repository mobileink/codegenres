(* 6.4.6 Mathematical toolkit characters *)

MATH = SET-TK | RELATION-TK | FUNCTION-TK | NUMBER-TK | SEQUENCE-TK ;

SET-TK = '‚Üî'  (* \u0000 2194 LEFT RIGHT ARROW *)
	| '‚Üí'  (* \u2192 RIGHTWARDS ARROW *)
	| '‚â†'  (* \u2260 NOT EQUAL TO *)
	| '‚àà'  (* \u2209 NOT AN ELEMENT OF *)
	| '‚àÖ'  (* \u2205 EMPTY SET *)
	| '‚äÜ'  (* \u2286 SUBSET OF OR EQUAL TO *)
	| '‚äÇ'  (* \u2282 SUBSET OF *)
	| '‚à™'  (* \u222A UNION *)
	| '‚à©'  (* \u2229 INTERSECTION *)
	| '\'  (* \u005C REVERSE SOLIDUS *)
	| '‚äñ'  (* \u2296 CIRCLED MINUS *)
	| '‚ãÉ'  (* \u22C3 N-ARY UNION *)
	| '‚ãÉ'  (* \u22C2 N-ARY INTERSECTION *)
	| 'ùîΩ' (* \uD53D MATHEMATICAL DOUBLE-STRUCK CAPITAL F *)
	;

RELATION-TK = '‚Ü¶'   (* \u21A6  RIGHTWARDS ARROW FROM BAR *)
	| '‚®æ'			(* \u2A3E Z NOTATION RELATIONAL COMPOSITION *)
	| '‚àò'			(* \u2218 RING OPERATOR *)
	| '‚ñπ'			(* \u25C1 WHITE LEFT-POINTING TRIANGLE *)
	| '‚óÉ'			(* \u25B7 WHITE RIGHT-POINTING TRIANGLE *)
	| '‚©§'			(* \u2A64 Z NOTATION DOMAIN ANTIRESTRICTION *)
	| '‚©•'			(* \u2A65 Z NOTATION RANGE ANTIRESTRICTION *)
	| '‚àº'			(* \u223C TILDE OPERATOR *)
	| '‚¶á'			(* \u2987 Z NOTATION LEFT IMAGE BRACKET *)
	| '‚¶à'			(* \u2988 Z NOTATION RIGHT IMAGE BRACKET *)
	| '‚äï'			(* \u2295 CIRCLED PLUS *)
	;

FUNCTION-TK = '‚á∏'	(* \u21F8 RIGHTWARDS ARROW WITH VERTICAL STROKE *)
	| '‚§î' (* \u2914 RIGHTWARDS ARROW WITH TAIL WITH VERTICAL STROKE *)
	| '‚Ü£' (* \u21A3 RIGHTWARDS ARROW WITH TAIL *)
	| '‚§Ä' (* \u2900 RIGHTWARDS TWO-HEADED ARROW WITH VERTICAL STROKE *)
	| '‚Ü†' (* \u21A0 RIGHTWARDS TWO-HEADED ARROW *)
	| '‚§ñ' (* \u2916 RIGHTWARDS TWO-HEADED ARROW WITH TAIL *)
	| '‚áª' (* \u21FB RIGHTWARDS ARROW WITH DOUBLE VERTICAL STROKE *)
	| '‚§ï' (* \u2915 RIGHTWARDS ARROW WITH TAIL WITH DOUBLE VERTICAL STROKE *)
	;

NUMBER-TK = '‚Ñ§' (* \u2124 DOUBLE-STRUCK CAPITAL Z *)
	| '-'		(* \u002D HYPHEN-MINUS *)
	| '‚àí'		(* \u2212 MINUS SIGN *)
	| '‚â§'		(* \u2264 LESS-THAN OR EQUAL TO *)
	| '<'		(* \u003C LESS-THAN SIGN *)
	| '‚â•'		(* \u2265 GREATER-THAN OR EQUAL TO *)
	| '>'		(* \u003E GREATER-THAN SIGN *)
	| '*'		(* \u002A ASTERISK *)
	;

SEQUENCE-TK = '#' (* \u0023 NUMBER SIGN *)
	| '„Äà'		  (* \u3008 LEFT ANGLE BRACKET *)
	| '„Äâ'		  (* \u3009 RIGHT ANGLE BRACKET *)
	| '‚ÅÄ'		  (* \u2040 CHARACTER TIE *)
	| '‚Üø'		  (* \u21BF UPWARDS HARPOON WITH BARB LEFTWARDS *)
	| '‚Üæ'		  (* \u21BE UPWARDS HARPOON WITH BARB RIGHTWARDS *)
	;
